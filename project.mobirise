ayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container-fluid\">\n        <div class=\"row\">\n            <div class=\"col-md-10 col-lg-7\">\n              <div class=\"wrapper-left\" mbr-class=\"{'align-center' : alignContent == 2, 'align-right' : alignContent == 3}\">\n                  <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Ropita para bebe<br></h1> <br>\n                  <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showSubTitle\" data-app-selector=\".mbr-section-subtitle\">Arcaviva</h3>\n              </div>\n                \n                <div class=\"block\" mbr-class=\"{'justify-content-center' : alignContent == 2, 'justify-content-end' : alignContent == 3}\">\n                  <div mbr-buttons mbr-theme-style=\"display-7\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-primary\" href=\"https://www.facebook.com/arcaviva.confecciones.5\" data-app-placeholder=\"Type Text\" target=\"_blank\">facebook</a></div>\n                  <div class=\"number\">\n                    <a href=\"#\" class=\"icon-btn\" data-app-placeholder=\"Type Text\"><span mbr-icon class=\"mbrib-mobile  mbr-iconfont2 mbr-iconfont mbr-iconfont-btn\"></span></a>\n                    <p class=\"telephone align-center mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".telephone\"><b>3125921262</b></p>\n                  </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div mbr-if=\"showArrow\" class=\"mbr-arrow hidden-sm-down\" aria-hidden=\"true\">\n        <a hre